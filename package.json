{
  "name": "ai3-release-manager",
  "version": "1.0.0",
  "description": "Download assets from GitHub releases based on network types and asset mappings",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "ai3-release-manager": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc && tsc-alias",
    "start": "node dist/index.js",
    "dev": "ts-node -r tsconfig-paths/register src/index.ts",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "test": "echo \"No tests yet\"",
    "check-update": "ts-node -r tsconfig-paths/register src/utils/update-checker.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/monkeyscanjump/ai3-release-manager.git"
  },
  "keywords": [
    "github",
    "release",
    "assets",
    "download",
    "cli"
  ],
  "author": "Monkey/WHat",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.2",
    "cheerio": "^1.0.0-rc.12",
    "semver": "^7.5.4"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.0.2",
    "@eslint/js": "^8.56.0",
    "@types/node": "^20.10.4",
    "@types/semver": "^7.5.3",
    "eslint": "^9.0.0-alpha.0",
    "prettier": "^3.1.1",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.11",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3",
    "glob": "^10.3.10",
    "rimraf": "^5.0.5"
  },
  "optionalDependencies": {
    "@pm2/io": "^5.0.2"
  },
  "overrides": {
    "glob": "^10.3.10",
    "rimraf": "^5.0.5",
    "@humanwhocodes/config-array": "npm:@eslint/config-array@latest",
    "@humanwhocodes/object-schema": "npm:@eslint/object-schema@latest",
    "inflight": "npm:lru-cache@^10.2.0",
    "uuid": "^9.0.1"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  }
}
